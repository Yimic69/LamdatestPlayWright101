<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Suite" parallel="tests" thread-count="4">
	<test name="Test in Chromium+Windows 10">
		<parameter name="browserName" value="pw-chromium" />
		<parameter name="browserVersion" value="latest" />
		<parameter name="platform" value="Windows 10" />
		<classes>
			<class name="Playwright.PlaywrightTest101"></class>
		</classes>
	</test>
	
	<test name="Test in Firefox+macOS Catalina">
		<parameter name="browserName" value="pw-firefox" />
		<parameter name="platform" value="macOS Catalina" />
		<parameter name="browserVersion" value="latest" />
		<classes>
			<class name="Playwright.PlaywrightTest101"></class>
		</classes>
	</test>
	 
	<test name="Test in Firefox+macOS Sequoia">
		<parameter name="browserName" value="pw-firefox" />
		<parameter name="platform" value="macOS Sequoia " />
		<parameter name="browserVersion" value="latest" />
		<classes>
			<class name="Playwright.PlaywrightTest101"></class>
		</classes>
	</test>
	
</suite>